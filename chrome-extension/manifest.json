{
	"manifest_version" : 2,
	"name": "PicMonkey > WordPress",
	"version": "1.0",
	"description": "send to PicMonkey screenshots and auto-save to WordPress",
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"default_icon": "icon_16x16.png",
		"default_title": "PicMonkey -> WordPress",
		"default_popup": "popup.html"
	},
	"icons": {
		"128": "icon_128x128.png",
		"16": "icon_16x16.png",
		"32": "icon_32x32.png",
		"48": "icon_48x48.png"
	},
	"options_page": "options.html",
	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*",
		"http://*.picmonkey.com/",
		"https://*.picmonkey.com/"
	]
}

